{
  "name": "@chrisrydahl/postman",
  "version": "0.0.4",
  "private": true,
  "description": "This package is used to update a postman collection with the latest version of a github repository.",
  "main": "lib/main.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "git add . && git commit -m \"Updates\" && npm version patch && pnpm build && pnpm format && pnpm lint && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisrydahl/postman.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.2.1",
    "octokit": "^2.0.10"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
